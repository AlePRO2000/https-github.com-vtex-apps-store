{
  "store.home": {
    "path": "/"
  },
  "store.account": {
    "path": "/account"
  },
  "store.login": {
    "path": "/login"
  },
  "store.product": {
    "path": "/_v/segment/:slug/p(/:id)",
    "canonical": "/:slug/p",
    "contentType": "product"
  },
  "store.search": {
    "path": "/:term/s",
    "canonical": "/:term"
  },
  "store.search#brand": {
    "path": "/_v/segment/:brand/b(/:id)",
    "canonical": "/:brand",
    "map": ["b"],
    "contentType": "brand"
  },
  "store.search#department": {
    "path": "/_v/segment/:department/d(/:id)",
    "canonical": "/:department",
    "map": ["c"],
    "contentType": "department"
  },
  "store.search#category": {
    "path": "/_v/segment/:department/:category(/c/:id)",
    "canonical": "/:department/:category",
    "map": ["c", "c"],
    "contentType": "category"
  },
  "store.search#subcategory": {
    "path": "/_v/segment/:department/:category/:subcategory(/c/:id)",
    "canonical": "/:department/:category/:subcategory",
    "map": ["c", "c", "c"],
    "contentType": "subcategory"
  },
  "store.search#subcategory-terms": {
    "path": "/_v/segment/:department/:category/:subcategory/*terms",
    "canonical": "/:department/:category/:subcategory/*terms"
  },
  "store.search#configurable": {
    "path": "/s/*terms"
  },
  "store.orderplaced": {
    "path": "/checkout/orderPlaced"
  }
}
