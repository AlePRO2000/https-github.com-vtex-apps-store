{
  "store": {
    "allowConditions": false,
    "component": "*",
    "around": [],
    "before": [],
    "after": [],
    "allowed": [
      "notification.bar",
      "carousel",
      "shelf",
      "rebuy",
      "promo-bar",
      "info-card",
      "newsletter",
      "rich-text",
      "flex-layout",
      "sandbox",
      "image",
      "sticky-layout"
    ]
  },
  "store.home": {
    "allowed": ["search-result"],
    "preview": {
      "type": "box",
      "height": 1400
    }
  },
  "store.product": {
    "allowed": [
      "product-add-to-list-button",
      "product-details",
      "product-kit",
      "product-reviews",
      "product-questions-and-answers",
      "product-rating-summary",
      "product-rating-inline",
      "product-images",
      "product-brand",
      "product-name",
      "product-price",
      "sku-selector",
      "buy-button",
      "product-separator",
      "product-description",
      "product-specifications",
      "product-quantity",
      "breadcrumb",
      "shipping-simulator",
      "availability-subscriber",
      "share",
      "product-highlights",
      "product-availability",
      "product-identifier",
      "blog-related-posts",
      "product-assembly-options",
      "product-teaser.product"
    ],
    "context": "ProductContext",
    "preview": {
      "type": "box",
      "height": 800
    }
  },
  "store.search": {
    "required": ["search-result"],
    "context": "SearchContext",
    "preview": {
      "type": "grid",
      "height": 1400
    }
  },
  "store.account": {
    "required": ["my-account"],
    "preview": {
      "type": "text",
      "height": 800
    }
  },
  "store.login": {
    "required": ["login-content"],
    "preview": {
      "type": "spinner",
      "height": 600
    }
  },
  "store.orderplaced": {
    "required": ["order-placed"],
    "allowed": ["shop-review-form"],
    "preview": {
      "type": "spinner",
      "height": 600
    }
  },
  "store.product-review-form": {
    "allowed": ["product-review-form"],
    "preview": {
      "type": "spinner",
      "height": 800
    }
  },
  "storeWrapper": {
    "allowed": ["highlight-overlay"],
    "component": "StoreWrapper"
  },
  "homeWrapper": {
    "component": "HomeWrapper"
  },
  "productWrapper": {
    "component": "ProductWrapper"
  },
  "searchWrapper": {
    "component": "SearchWrapper"
  },
  "customWrapper": {
    "component": "CustomWrapper"
  },
  "challenge": {
    "component": "DefaultChallenge",
    "extensible": "public"
  },
  "highlight-overlay": {
    "component": "*",
    "extensible": "vtex"
  },
  "challenge.profile": {
    "component": "ProfileChallenge"
  },
  "promo-bar": {
    "component": "*"
  },
  "store.custom": {
    "allowed": ["search-result"]
  },
  "store.content": {
    "context": "ContentPageContext"
  }
}
